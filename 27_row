using System;

namespace _27_row
{
    class Program
    {
        static void Main(string[] args)
        {
            Random r = new Random();
            for(int i = 0; i < 20; i++)
            {
                int player = r.Next(21);
                if(player % 2 == 0)
                {
                    Console.WriteLine("O player " + i + " tem um nivel par de " + player);
                }
            }
        }
    }
}
